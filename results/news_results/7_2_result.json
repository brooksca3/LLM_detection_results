        },
        {
          "generated_prob": 0.00011115506640635431,
          "perplexity": 302,
          "sentence": "Paul died in 1994 from a medical Procedure gone wrong.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00011159518908243626,
          "perplexity": 247,
          "sentence": "I'll be buried next to Paul.'",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012194273585919291,
          "perplexity": 26,
          "sentence": "The former soldier explained that he had never dared reveal his secret for fear of being disowned by those he knew and loved.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00011937025556107983,
          "perplexity": 21,
          "sentence": "'I'm sorry for not having the courage to come out as Gay.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012020637950627133,
          "perplexity": 31,
          "sentence": "I was afraid of being ostracized: by Family, Friends, and Co-Workers,' he recalled.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012354525097180158,
          "perplexity": 28,
          "sentence": "'Seeing how people like me were treated, I just could not do it.",
          "highlight_sentence_for_ai": false
        },
        {
