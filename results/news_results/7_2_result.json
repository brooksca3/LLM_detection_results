        },
        {
          "generated_prob": 0.00012309840531088412,
          "perplexity": 30,
          "sentence": "A Vietnam veteran kept a heartbreaking secret his entire life, only revealing it to his friends and family in his obituary.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012092100223526359,
          "perplexity": 16,
          "sentence": "Col. Edward Thomas Ryan died at the age of 85 in his home in Albany, New York, after living in nearby Rensselaer for most of his life, where he served as a firefighter.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00011973413347732276,
          "perplexity": 32,
          "sentence": "In a final message to his loved ones and the world, published in the Albany Times Union, Ryan shared he had secretly lived as a gay man his entire life.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012009936472168192,
          "perplexity": 23,
          "sentence": "'I must tell you one more thing.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00011143440497107804,
          "perplexity": 63,
          "sentence": "I was Gay all my life: thru grade school, thru High School, thru College, thru Life,' the veteran wrote.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00011096688103862107,
          "perplexity": 33,
          "sentence": "Ryan told of his 25-year relationship with the love of his life, another man named Paul.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012095397687517107,
          "perplexity": 43,
          "sentence": "Col. Edward Thomas Ryan kept a heartbreaking secret his entire life, only revealing it to his friends and family in his obituary",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00011861094390042126,
          "perplexity": 131,
          "sentence": "'I was in a loving and caring relationship with Paul Cavagnaro of North Greenbush,' Ryan confessed.",
          "highlight_sentence_for_ai": false
        },
        {
          "generated_prob": 0.00012453443196136504,
          "perplexity": 19,
